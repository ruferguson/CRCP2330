/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Nand(a = b, b = sel, out = bSel);
    Nand(a = sel, b = sel, out = selSel);
    Nand(a = selSel, b = a, out = selA);
    Nand(a = bSel, b = selA, out = out);
}